Algoritmo "semnome"

Var

n1, n2, soma:  inteiro


Inicio
 soma <- 0
   Escreva ("Digite um número inteiro ")
   leia (n1)
   Escreva ("Digite outro número inteiro! ")
   leia (n2)
   n1 <- n1 + 1
   Enquanto  (n1<>n2) faca
   Escreval (n1)
   soma <- soma + n1
   n1 <- n1 + 1
   fimenquanto
   Escreva ("A soma dos valores é: ", soma)

Fimalgoritmo